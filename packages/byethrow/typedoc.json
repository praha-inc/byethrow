{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "tsconfig": "./tsconfig.build.json",
  "plugin": ["typedoc-plugin-markdown"],
  "router": "kind",
  "readme": "none",
  "indexFormat": "table",
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "categoryOrder": [
    "Core Types",
    "Infer Types",
    "Creators",
    "Combinators",
    "Unwraps",
    "Assertions",
    "Type Guards",
    "Utilities",
    "*"
  ]
}
