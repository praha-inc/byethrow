    "url": "https://github.com/praha-inc/byethrow/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/praha-inc/byethrow.git",
    "directory": "packages/docs"
  },
  "license": "MIT",
  "author": "PrAha, Inc.",
  "sideEffects": false,
  "type": "module",
  "exports": {
