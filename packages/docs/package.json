{
  "name": "@praha/byethrow-docs",
  "version": "0.9.0",
  "description": "Documentation for @praha/byethrow",
  "keywords": [
    "javascript",
    "typescript",
    "throw",
    "error",
    "functional",
    "documents"
  ],
  "homepage": "https://github.com/praha-inc/byethrow/blob/main/packages/docs/README.md",
